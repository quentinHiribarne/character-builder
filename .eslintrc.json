{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:vue/essential"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true
        }
    },
    "plugins": ["vue"],
    "rules": {
        "semi": ["warn", "always"],
        "quotes": ["warn", "double"],
        "no-unused-vars": "off"
    }
}
